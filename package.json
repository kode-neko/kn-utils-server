{
  "name": "kn-utils-server",
  "version": "0.0.0",
  "description": "It's a node-express app for several porpouses. At the moment only for a e-mail server",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "npm install && npm run build && npm run prod",
    "start:fast": "npm run build && npm run prod",
    "build": "tsc --project tsconfig.json",
    "build:dev": "tsc --watch --project tsconfig.json",
    "prod": "node ./public/src -m prod",
    "dev": "nodemon ./public/src -m dev",
    "test": "jest"
  },
  "repository": "git+https://github.com/kode-neko/kn-utils-server.git",
  "keywords": [
    "server",
    "mail"
  ],
  "author": "Kodeneko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kode-neko/kn-utils-server/issues"
  },
  "homepage": "https://github.com/kode-neko/kn-utils-server#readme",
  "dependencies": {
    "@types/chalk": "^2.2.0",
    "@types/express": "^4.17.14",
    "@types/express-pino-logger": "^4.0.3",
    "@types/figlet": "^1.5.5",
    "@types/helmet": "^4.0.0",
    "@types/nodemailer": "^6.4.6",
    "chalk": "^5.1.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-pino-logger": "^7.0.0",
    "figlet": "^1.5.2",
    "helmet": "^6.0.0",
    "nodemailer": "^6.8.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.8.4"
  }
}
