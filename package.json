{
  "name": "utils-server",
  "version": "0.0.0",
  "description": "It's a node-express app for several porpouses. At the moment only for a e-mail server",
  "main": "build/index.js",
  "scripts": {
    "dev": "webpack serve --mode development --config ./webpack.config.dev.js",
    "start": "node ./lib/index.js",
    "build": "npm run clean && tsc --project ./tsconfig.json",
    "start:dev": "nodemon ./lib/index.js",
    "build:dev": "tsc --watch --project ./tsconfig.json",
    "dist": "npm run clean && webpack --config ./webpack.config.js",
    "clean": "rm -rf ./dist ./lib",
    "test": "jest"
  },
  "repository": "git+https://github.com/kode-neko/kn-utils-server.git",
